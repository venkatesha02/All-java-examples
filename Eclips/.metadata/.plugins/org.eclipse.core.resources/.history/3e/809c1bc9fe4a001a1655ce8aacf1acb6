package com.tyss.jdbcapp.common;

public class TestB {

	public static String reverseWord(String str) {
		String word[] = str.split("\s");
		String result = "";
		for (int i = word.length-1; i >=0; i--) {
			result = result+word[i];
			
		}
		return result;
	}
	
	
	
	public static void main(String[] args) {

		
	}

}
